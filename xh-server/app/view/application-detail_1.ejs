                  } else if (tempItem.type == 'article_zuozhe' && tempItem.value) {
                    value = '';
                    for (var m = 0; m < tempItem.value.length; m++) {
                      value += (tempItem.value[m].key + ' ');
                    }
                  }
                  if (value[0] && value[0].label) {
                    value = value[0].label;
                  }
                  showVal += value + dotFlg;
                })
                cataData[attr].push({
                  extra: showVal,
                  files: fileId,
                });
              } else {
